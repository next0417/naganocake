<h2>商品一覧全<%= @items.count %>件</h2>
<% @items.each do |f| %>

<table>
  <tr>
    <td>
      <%= link_to item_path(f.id) do %>
      <%= image_tag f.get_image(300, 300) %><br>
      <% end %>
      <%= f.name %><br>
      ￥<%= f.price %>
    </td>
  </tr>
  <% end %>
  <%= paginate @items %>
</table>
