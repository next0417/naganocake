<h2>配送先編集</h2>
<%= form_with model: @address, url: address_path do |f| %>
<table>
  <tr>
    <th>郵便番号</th>
    <th>住所</th>
    <th>宛名</th>
  </tr>
  <tr>
    <td><%= f.text_field :postal_code, autofocus: true, autocomplete: "postal_code" %></td>
    <td><%= f.text_field :address, autofocus: true, autocomplete: "address" %></td>
    <td><%= f.text_field :name, autofocus: true, autocomplete: "name" %></td>
  </tr>
</table>
<%= f.submit "変更内容を保存", class: 'btn btn-success' %>
<% end %>
